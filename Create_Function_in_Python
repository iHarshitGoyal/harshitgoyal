def computegrade(a):
    if(a>=0.0 and a<=1.0):
        if(a>=0.9):
            return 'A'
        elif(a>=0.8):
            return 'B'
        elif(a>=0.7):
            return 'C'
        elif(a>=0.6):
            return 'D'
        elif(a<0.6):
            return 'F'
    else:
        return 'Bad Score'

s=input('Enter Your Score: ')
try:
    sf=float(s)
    score=computegrade(sf)
    print(score)
except:
    print('Bad Score')
